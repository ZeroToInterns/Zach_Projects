
public class ElectronicsStore {

	public static double getPrice(String item) {
		if (item == "Ipad") {
			return 350.99;
		}
		else if (item == "Macbook") { 
			return 1100.00;
		}
		else if (item == "Iphone") { 
			return 600.00;
		}
		else if (item == "TV") {
			return 1525.30;
		}
		else if (item == "Charger") {
			return 15.75;
		}
		else { 
			return 0.00;
		}
	}
}
