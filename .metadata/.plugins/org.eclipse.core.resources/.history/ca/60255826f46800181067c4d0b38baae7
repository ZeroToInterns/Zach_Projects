
public class person {

		//instance variables
		private static String name = "Michael"; 
		private static int age = 22; 
		private static String hairColor = "Black";
		private static String eyeColor = "Black";
		private static double weight = 160; 
		private static String height = "5'4"; 
		private static char gender = 'M'; 
		
		//main method 
		public static void main (String[] args) {
			//creates instance of person called newPerson 
			person newPerson = new person(); 
			System.out.println("Name =" + newPerson.name);
			System.out.println(newPerson.age);
			System.out.println(newPerson.hairColor);
			System.out.println(newPerson.eyeColor);
			System.out.println(newPerson.weight);
			System.out.println(newPerson.height);
			System.out.println(newPerson.gender);
		}
}
