
public class ElectronicsStore {

	public static double getPrice(String item) {
		double price;
		
		if (item == "Ipad") {
			price = 300.00;
		}
		else if (item == "Macbook") { 
			price = 1000.00;
		}
		else if (item == "Iphone") { 
			price = 600.00;
		}
		else if (item == "TV") {
			price = 1500.00;
		}
		else if (item == "Charger") {
			price = 15.00;
		}
		else { 
			price = 0.00;
		}
		return price; 
	}
}
